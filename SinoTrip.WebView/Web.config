<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
    <section name="RewriterConfig" type="URLRewriter.Config.RewriterConfigSerializerSectionHandler,URLRewriter"/>
  </configSections>
  <connectionStrings configSource="App_Config\Database.config"/>
  <appSettings configSource="App_Config\Setting.config"/>
  <RewriterConfig>
    <Rules>
      <RewriterRule>
        <LookFor>/818.html</LookFor>
        <SendTo>/Act/201408/818/default.aspx</SendTo>
      </RewriterRule>


      <RewriterRule>
        <LookFor>/news/([0-9]*).html</LookFor>
        <SendTo>/news/info.aspx?id=$1</SendTo>
      </RewriterRule>


      <RewriterRule>
        <LookFor>/help/</LookFor>
        <SendTo>/help/default.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/helps/([0-9]*).html</LookFor>
        <SendTo>/help/info.aspx?id=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/visa.html</LookFor>
        <SendTo>/mall/Visa/Default.aspx</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/visa/index.html</LookFor>
        <SendTo>/mall/Visa/List.aspx</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/visa/ly/index.html</LookFor>
        <SendTo>/mall/Visa/List.aspx?type=370</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/visa/biz/index.html</LookFor>
        <SendTo>/mall/Visa/List.aspx?type=371</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/visa/([0-9]*).html</LookFor>
        <SendTo>/mall/visa/info.aspx?id=$1</SendTo>
      </RewriterRule>
      <RewriterRule>

        <LookFor>/sale/([0-9]*)/([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/sale/info.aspx?forum=$1&amp;id=$2&amp;productid=$3&amp;planid=$4</SendTo>
      </RewriterRule>

      <RewriterRule>

        <LookFor>/tejia.html</LookFor>
        <SendTo>/mall/tejia.aspx</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?page=$1&amp;type=0&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/productSearch.html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?type=0</SendTo>
      </RewriterRule>


      <RewriterRule>
        <LookFor>/around.html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?type=272</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/Abroad.html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?type=272</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/domestic.html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?type=283</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/Tour/([0-9]*)\.html</LookFor>
        <SendTo>/mall/Tour/TourItem.aspx?tourid=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/tour([0-9]*)\.html</LookFor>
        <SendTo>/mall/Tour/TourItem.aspx?eid=$1</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/domestic/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html?(.*)</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?page=$1&amp;type=283&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6&amp;$7</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/domestic/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?page=$1&amp;type=283&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/Abroad/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/mall/Tour/TourList.aspx?page=$1&amp;type=272&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6</SendTo>
      </RewriterRule>






      <RewriterRule>
        <LookFor>/Free/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/mall/Tour/Free.aspx?page=$1&amp;type=0&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/Free/domestic/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/mall/Tour/Free.aspx?page=$1&amp;type=283&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6</SendTo>
      </RewriterRule>


      <RewriterRule>
        <LookFor>/Free/Abroad/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/mall/Tour/Free.aspx?page=$1&amp;type=272&amp;area=$2&amp;areaplay=$3&amp;tratype=$4&amp;journeytype=$5&amp;hotletype=$6</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/Free/default.html</LookFor>
        <SendTo>/mall/Tour/Free.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/Free.html</LookFor>
        <SendTo>/mall/Tour/Free.aspx</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/Free/domestic.html</LookFor>
        <SendTo>/mall/Tour/Free.aspx?type=283</SendTo>
      </RewriterRule>


      <RewriterRule>
        <LookFor>/Free/Abroad.html</LookFor>
        <SendTo>/mall/Tour/Free.aspx?type=272</SendTo>
      </RewriterRule>

      <RewriterRule>
        <LookFor>/Hotel/([a-zA-Z0-9]*)/([a-zA-Z0-9\-]*)/([a-zA-Z0-9\-]*)/([a-zA-Z0-9]*)\.html</LookFor>
        <SendTo>/mall/Hotel/HotelItem.aspx?inDate=$2&amp;outDate=$3&amp;hotlecode=$4&amp;city=$1</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/Scenic/Item([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/Scenic/Item.aspx?id=$1&amp;outsign=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/Scenic.html</LookFor>
        <SendTo>/Scenic/List.aspx</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>/Scenic/([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*)-([0-9]*).html</LookFor>
        <SendTo>/Scenic/List.aspx?provice=$1&amp;city=$2&amp;county=$3&amp;type=$4&amp;price=$5&amp;grade=$6&amp;page=$7</SendTo>
      </RewriterRule>
    </Rules>
  </RewriterConfig>
  <system.web>
    <!--
    <customErrors mode="On" defaultRedirect="/404.htm">
      <error statusCode="403" redirect="/500.htm"/>
      <error statusCode="404" redirect="/404.htm"/>
      <error statusCode="500" redirect="/500.htm"/>
    </customErrors>-->
    
    <compilation debug="true" targetFramework="4.0">
      <buildProviders>
        <add extension=".html" type="System.Web.Compilation.PageBuildProvider" />
      </buildProviders>
    </compilation>

    <httpRuntime requestValidationMode="2.0"/>
  </system.web>
  <system.webServer>
    <handlers>
      <add verb="*" path="*.html" type="URLRewriter.RewriterFactoryHandler, URLRewriter" name="htmlRewriter" />
    </handlers>
  </system.webServer>
</configuration>